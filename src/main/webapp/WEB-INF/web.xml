<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>getUploadPercent</servlet-name>
		<servlet-class>com.wp.servlet.GetUploadPercent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>uploadServlet</servlet-name>
		<servlet-class>com.wp.servlet.UploadServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>getUploadPercent</servlet-name>
		<url-pattern>/servlet/getUploadPercent</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>uploadServlet</servlet-name>
		<url-pattern>/servlet/uploadServlet</url-pattern>
	</servlet-mapping>
</web-app>